{{#each post as |post|}}
  <a href="/dashboard/update/{{id}}">
    <div class="card">
      <h3 class="card-header">
        <div style="float: left;">{{title}}</div>
        <div style="font-weight: normal; float: right;">Posted on {{formatDate date_created 'MM/DD/YYYY'}}</div>
      </h3>
    </div>
  </a>
{{/each}}

<button class="btn" style="background-color: #a587f0;" id="create_post" type="button">New Post</button>

<script src="/js/dashboard.js"></script>